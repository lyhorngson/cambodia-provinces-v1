{
    "version": 2,
    "builds": [
      {
        "src": "dist/main.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/province/(.*)",
        "dest": "/public/data/province/$1.json"
      },
      {
        "src": "/provinces",
        "dest": "/public/data/provinces.json"
      },
      {
        "src": "/(.*)",
        "dest": "/dist/main.js"
      }
    ]
  }